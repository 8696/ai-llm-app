  "main": "index.js",
  "scripts": {
    "client:start": "cross-env CUSTOM_ENV=development node scripts/start.js",
    "client:build": "rimraf dist-client && yarn run client:eslint && cross-env CUSTOM_ENV=production node scripts/build.js",
    "client:analyze": "yarn run client:eslint && cross-env CUSTOM_ENV=production ANALYZE=true node scripts/build.js",
    "client:eslint": "cross-env NODE_ENV=production eslint --fix ./src",
    "server:start": "nodemon --watch ./server-src --ext ts --exec 'cross-env NODE_ENV=development FEATURE_ENV=development ts-node --project ./tsconfig.server.json ./server-src/app.ts'",
    "server:build": "rimraf dist-server && yarn server:eslint && tsc --project ./tsconfig.server.json",
    "server:eslint": "cross-env NODE_ENV=production PROECJT_TYPE=server eslint --fix ./server-src",
    "pm2:start": "yarn server:build && yarn pm2:delete && pm2 start pm2.config.js --env production",
    "pm2:stop": "pm2 stop pm2.config.js",
    "pm2:restart": "pm2 restart pm2.config.js",
    "pm2:delete": "pm2 delete pm2.config.js",
    "prepare": "husky install",
    "ncu:check": "ncu --target minor",
    "ncu:update": "ncu --target minor -u"
  },
